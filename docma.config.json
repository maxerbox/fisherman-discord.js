{
    "src": [
        {
            "fisherman": "./lib/**/*.js"
        },
        {
            "guide": "./HOME.md",
            "tutorial": "./TUTORIAL.md"
        },
        {
            "middleware_tutorial": "./MIDDLEWARE.md",
            "middleware_list": "./middlewareLIST.md"
        }
    ],
    "dest": "./docs",
    "debug": true,
    "jsdoc": {
        "package": "./package.json"
    },
    "app": {
        "title": "Fisherman discord.js",
        "server": "static",
        "base": "/fisherman-discord.js",
        "entrance": "content:guide",
        "meta": [
            {
                "property": "og:title",
                "content": "Fisherman - discord.js"
            },
            {
                "property": "og:description",
                "content": "A lightweight, fast, and powerful discord command router writed in javascript standard style and inspired by expressjs"
            }
            ,
            {
                "property": "og:image",
                "content": "https://cdn.rawgit.com/maxerbox/fisherman-discord.js/dfc8afbe/logo.png"
            }
        ]
    },
    "template": {
        "options": {
            "outline": "tree",
            "symbolMeta": true,
            "title": "Docs",
            "navItems": [
                {
                    "iconClass": "ico-book",
                    "label": "Fisherman Docs",
                    "items": [
                        {
                            "label": "Home",
                            "href": "./"
                        },
                        {
                            "label": "Quick tutorial",
                            "href": "?content=tutorial"
                        },
                        {
                            "separator": true
                        },
                        {
                            "label": "Api docs",
                            "href": "?api=fisherman"
                        }
                    ]
                },
                {
                    "iconClass": "ico-terminal",
                    "label": "Middlewares",
                    "items": [
                        {
                            "label": "Creating a middleware",
                            "href": "?content=middleware_tutorial"
                        },
                        {
                            "label": "Middleware list",
                            "href": "?content=middleware_list"
                        }
                    ]
                },
                {
                    "iconClass": "ico-reply",
                    "label": "Discord server",
                    "href": "https://github.com/maxerbox/fisherman-discord.js",
                    "target": "_blank"
                },
                {
                    "iconClass": "ico-md ico-github",
                    "label": "GitHub",
                    "href": "https://github.com/maxerbox/fisherman-discord.js",
                    "target": "_blank"
                }
            ]
        }
    }
}