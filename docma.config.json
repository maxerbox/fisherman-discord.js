{
    "src": [
        {
            "fisherman": "./lib/**/*.js"
        },
        {
            "guide": "./README.md",
            "tutorial": "./docs/tutorial.md"
        },
        {
            "middleware_tutorial" : "./docs/middleware.md",
            "middleware_list": "./docs/middlewareList.md"
        }
    ],
    "dest": "./docs",
    "debug": true,
    "jsdoc": {
        "package": "./package.json"
    },
    "app": {
        "title": "Fisherman discord.js",
        "server": "static",
        "base": "/fisherman-discord.js",
        "entrance": "content:guide"
    },
    "template": {
        "options": {
            "outline": "tree",
            "symbolMeta": true,
            "title": "Docs",
            "navItems": [
                {
                    "iconClass": "ico-book",
                    "label": "Fisherman Docs",
                    "items": [
                        {
                            "label": "Home",
                            "href": "./"
                        },
                        {
                            "label": "Quick tutorial",
                            "href": "?content=tutorial"
                        },
                        {
                            "separator": true
                        },
                        {
                            "label": "Api docs",
                            "href": "?api=fisherman"
                        }
                    ]
                },
                {
                    "iconClass": "ico-terminal",
                    "label": "Middlewares",
                    "items": [
                        {
                            "label": "Creating a middleware",
                            "href": "?content=middleware_tutorial"
                        },
                        {
                            "label": "Middleware list",
                            "href": "?content=middleware_list"
                        }
                    ]
                },
                {
                    "iconClass": "ico-md ico-github",
                    "label": "GitHub",
                    "href": "https://github.com/maxerbox/fisherman-discord.js",
                    "target": "_blank"
                }
            ]
        }
    }
}